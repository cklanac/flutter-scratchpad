  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Debug",
      // "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--verbose"
      ]
    },
    {
      // Profile Mode - App CANNOT use default com.example.<app_name>
      // Must change signing id to custom (ex com.chicagoworks.<app_name>)
      "name": "Flutter Profile",
      "program": "lib/provider/breakdown/provider_layout_2.dart",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    }
  ]
}